    }
  ],
  "autoload": {
    "psr-4": {
      "Quiz\\": "app/"
    }
  },
  "require": {
    "php": "^8.3",
    "ext-dom": "*",
    "symfony/console": "^7.1",
    "symfony/dotenv": "^7.1",
    "symfony/framework-bundle": "^7.1",
    "symfony/orm-pack": "^2.4",
    "symfony/runtime": "^7.1",
    "symfony/yaml": "^7.1"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
